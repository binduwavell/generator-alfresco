{
  "name": "generator-alfresco",
  "version": "0.9.0",
  "description": "Yeoman generator",
  "license": "Apache-2.0",
  "main": "app/index.js",
  "repository": "binduwavell/generator-alfresco",
  "author": {
    "name": "Bindu Wavell",
    "email": "bindu@ziaconsulting.com",
    "url": "https://github.com/binduwavell"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "alfresco",
    "all-in-one",
    "generator",
    "yeoman",
    "yeoman-generator"
  ],
  "dependencies": {
    "adm-zip": "0.4.7",
    "chalk": "1.1.3",
    "cross-spawn": "2.2.3",
    "deasync": "0.1.4",
    "debug": "2.2.0",
    "lodash": "4.11.0",
    "pretty-data": "0.40.0",
    "rmdir": "1.2.0",
    "semver": "5.1.0",
    "split": "1.0.0",
    "window-size": "0.2.0",
    "wrap-ansi": "2.0.0",
    "xmldom": "0.1.22",
    "xpath": "0.0.23",
    "yeoman-generator": "0.22.5",
    "yosay": "1.1.1"
  },
  "devDependencies": {
    "coveralls": "2.11.9",
    "eslint": "2.7.0",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-promise": "1.1.0",
    "eslint-plugin-standard": "1.3.2",
    "fixme": "0.3.0",
    "ghooks": "1.2.1",
    "istanbul": "0.4.3",
    "mem-fs": "1.1.2",
    "mem-fs-editor": "2.2.0",
    "mocha": "2.4.5",
    "mocha-eslint": "2.0.2",
    "mocha-lcov-reporter": "1.2.0",
    "npm-check-updates": "2.6.1",
    "shelljs": "0.5.3",
    "yeoman-assert": "2.1.2",
    "yeoman-test": "1.1.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "eslint generators test"
    }
  },
  "scripts": {
    "eslint": "eslint generators test || echo DONE",
    "fixme": "node -e \"require('fixme')()\"",
    "ncu": "ncu",
    "ncu:upgrade": "ncu -u",
    "test": "mocha --reporter spec test",
    "test:color": "mocha --colors --reporter spec test",
    "test:one": "mocha --reporter spec",
    "test:watch": "mocha --reporter spec --watch test",
    "test:watchone": "mocha --reporter spec --watch",
    "howto-cover-local": "echo 'Place a repo_token: entry with the project secret from the coveralls dashboard into .coveralls.yml' ; echo 'WARNING: do not commit this secret!!!'",
    "cover": "istanbul cover _mocha -- --report spec test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover:color": "istanbul cover _mocha -- --colors --report spec test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover:local": "istanbul cover _mocha -- --report spec test ; node -e \"require('fixme')()\""
  }
}
